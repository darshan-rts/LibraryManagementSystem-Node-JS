 <%- include( "partials/header" ) -%> 
<div>
    <h3>Welcome   <%=username %>  to library managment system</h3>
    <table class="table">
  <thead>
    <tr>
      <th scope="col">BookId</th>
      <th scope="col">Name</th>
      <th scope="col">Author</th>
      <th scope="col">PublishDate</th>
      <th scope = 'col'>Stock</th>
    </tr>
  </thead>

  <tbody>
    <% bookdata.forEach(element => {%>
    <tr>
      <th scope="row"><%=element.book_id%></th>
      <td <%=element.bookname%>"><%=element.bookname%></td>
      <td><%=element.author%></td>
      <td><%=element.pub_date%></td>
      <td><%=element.stocks %> </td>
      <td><a class = "btn btn-primary" href="/login/home/addbook?id=<%=element.book_id%>&name=<%=element.bookname%>&author=<%=element.author%>&pub_date=<%=element.pub_date%>&stocks=<%=element.stocks %> " >
           Add Book</a></td>
      <input type="hidden" name="stock" value="<%=element.stocks %>">   
 </tr>
    <% }); %>
   
  </tbody>
</table>
  

</div>
<%- include( "partials/footer" ) -%> 